<!doctype html>
<html lang="pt">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>API com MVC NODE</title>
    <link href="https://getbootstrap.com/docs/4.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>

<body>
    <div class="container">
        <br /><br /><br />
        <h2>Exemplo API Node.JS com MVC</h2>
        <br />
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">COMPE</th>
                    <th scope="col">ShortName</th>
                    <th scope="col">Document</th>
                    <th scope="col">Type</th>
                    <th scope="col">Url</th>
                </tr>
            </thead>
            <tbody>
                <% if (dados.length> 0) { %>
                    <% for(var i=0; i < dados.length; i++) {%>
                        </tr>
                        <th scope="row">
                            <%= dados[i].COMPE %>
                        </th>
                        <td>
                            <h4>
                                <%= dados[i].ShortName %>
                            </h4>
                        </td>
                        <td>
                            <%= dados[i].Document %>
                        </td>
                        <td>
                            <%= dados[i].Type %>
                        </td>
                        <td>
                            <%= dados[i].Url %>
                        </td>
                        </tr>
                        <% } %>

                            <% } %>
    </div>
</body>

</html>